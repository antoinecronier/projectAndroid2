<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="2.5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="2.5dp" >
	    <TextView
			android:id="@+id/row_logtraca_produit"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	        android:layout_alignParentTop="true" />
	    <TextView
			android:id="@+id/row_logtraca_machine"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_logtraca_produit" />
	    <TextView
			android:id="@+id/row_logtraca_user"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_logtraca_machine" />
	    <TextView
			android:id="@+id/row_logtraca_duree"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_logtraca_user" />
	    <TextView
			android:id="@+id/row_logtraca_dateentre"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_logtraca_duree" />
	    <TextView
			android:id="@+id/row_logtraca_datesortie"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_logtraca_dateentre" />

</RelativeLayout>
